#include <iostream>
#include <string>
using namespace std;
class student
{
public:
    string name;
    int roll_no;
};

int main()
{
    student st1;
    st1.name ="john";
    st1.roll_no = 2;
    cout << "Name of the student: "<<st1.name<< endl;
    cout << "roll number: "<<st1.roll_no << endl;
}
